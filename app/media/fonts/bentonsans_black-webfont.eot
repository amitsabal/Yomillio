/**FreePattern pdf Dwonload tracking event **/
function trackFreePatternPDFDownloaded(patternName){
	ga('send', 'event', 'Free Patterns', 'PDF Download', patternName);
}
/**FreePattern Add to Favourite tracking event **/
function trackFreePatternAddToMyFavourites(patternName){
	ga('send', 'event', 'Free Patterns', 'Add to My Favourites', patternName);
}
/**Review Written submitted tracking event **/
function trackReviewWritten(patternName){
	ga('send', 'event', 'Review Written', 'Submitted', patternName);
}
/**mails click tracking event **/
function trackMails(emailAddresses){
	ga('send', 'event', 'Mails', 'Click', emailAddresses);
}
/**Outbound links click tracking event **/
function trackOutboundLinks(externalURLs){
	if(externalURLs != "#")
	{
		ga('send', 'event', 'Outbound links', 'Click', externalURLs);
	}
}
/**Video Plays click tracking event **/
function trackVideoPlays(videoName){
	ga('send', 'event', 'Video', 'Play', videoName);
}
/**Video ended tracking event **/
function trackVideoEnd(videoName){
	ga('send', 'event', 'Video', 'End', videoName);
}
/**Video pused tracking event **/
function trackVideoPause(videoName){
	ga('send', 'event', 'Video', 'Pused', videoName);
}
/**NewsLetter page view with newsletter checked tracking **/
function trackPgeViewWithNSU(){
	ga('send', 'pageview', {'page': '/account-registration-nsu','title': 'Account Registration with Newsletter'});
}
/** NewsLetter page view with newsletter not checked tracking **/
function trackPgeViewWithOutNSU(){
	ga('send', 'pageview', {'page': '/account-registration','title': 'Account Registration'});
}
/**NewsLetter signup pge view tracking **/
function trackPageNsignUP(){
	ga('send', 'pageview', {'page': '/newsletter-sign-up','title': 'Newsletter Sign Up'});
}
/** track Logout event**/
function trackLogout(){
	ga('send', 'event', 'Logout', 'Logout');
}


/** track DeliveryAddress event**/
function trackDeliveryAddress(){
	ga('ec:setAction','checkout', {'step': 1,'option': 'DeliveryAddress'});
	ga('send', 'pageview', {'title': 'DeliveryAddress'});
	
}
/** track DeliveryOptions event**/
function trackDeliveryOptions(){
	ga('ec:setAction','checkout', {'step': 2,'option': 'DeliveryOptions'});
	ga('send', 'pageview', {'title': 'DeliveryOptions'});
	
}
/** track PaymentDetail event**/
function trackPaymentDetails(){
	ga('ec:setAction','checkout', {'step': 3,'option': 'PaymentDetails'});
	ga('send', 'pageview', {'title': 'PaymentDetails'});
	
}
/** track FinalReview event**/
function trackFinalReview(){
	ga('ec:setAction','checkout', {'step': 4,'option': 'FinalReview'});
	ga('send', 'pageview', {'title': 'FinalReview'});
	
}

/** Function to remove special characters from String **/
function cleanSplChar(inputString){
                    var cleanString = inputString;
                    re = /[`~!@#$%^&*()_|+\-=?;:'",.<>\{\}\[\]\\\/]/gi;
                    var isSplChar = re.test(inputString);
                    if(isSplChar)
                    {
                    	cleanString = inputString.replace(/[`~!@#$%^&*()_|+\-=?;:'",.<>\{\}\[\]\\\/]/gi,'');
                    }
                    
                    return cleanString;
}




